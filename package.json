{
    "name": "cosmicdivision-gatsbyblog",
    "private": true,
    "description": "A personal blog starter for a blog powered by Netlify CMS, based on the default blog starter",
    "version": "0.1.0",
    "author": "Andre Clark <andrek@cosmicdivision.dev>",
    "bugs": {
        "url": "https://github.com/AndreBClark/cosmicdivision/issues"
    },
    "dependencies": {
        "@emotion/styled": "^11.3.0",
        "@openfonts/fira-code_latin": "^1.44.8",
        "@tailwindcss/typography": "^0.4.1",
        "gatsby": "^3.9.1",
        "gatsby-plugin-emotion": "^6.9.0",
        "gatsby-plugin-image": "^1.9.0",
        "gatsby-plugin-manifest": "^3.9.0",
        "gatsby-plugin-module-resolver": "^1.0.3",
        "gatsby-plugin-netlify": "^3.9.0",
        "gatsby-plugin-offline": "^4.9.0",
        "gatsby-plugin-postcss": "^4.9.0",
        "gatsby-plugin-preact": "^5.9.0",
        "gatsby-plugin-react-helmet": "^4.9.0",
        "gatsby-plugin-react-svg": "^3.0.1",
        "gatsby-plugin-sharp": "^3.10.2",
        "gatsby-plugin-transition-link": "^1.20.5",
        "gatsby-plugin-typography": "^3.9.0",
        "gatsby-remark-images": "^5.6.0",
        "gatsby-remark-responsive-iframe": "^4.6.0",
        "gatsby-remark-smartypants": "^4.6.0",
        "gatsby-remark-vscode": "^3.3.0",
        "gatsby-source-filesystem": "^3.9.0",
        "gatsby-transformer-remark": "^4.6.0",
        "gatsby-transformer-sharp": "^3.9.0",
        "gsap": "^3.7.0",
        "preact": "^10.5.14",
        "preact-render-to-string": "^5.1.19",
        "tailwindcss": "^2.2.4",
        "twin.macro": "^2.6.2",
        "typeface-fira-sans": "1.1.13",
        "typeface-orbitron": "^1.1.3"
    },
    "devDependencies": {
        "@emotion/react": "^11.4.0",
        "autoprefixer": "^10.3.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "cssnano": "^5.0.6",
        "cssnano-preset-advanced": "^5.1.3",
        "dotenv": "^10.0.0",
        "postcss": "^8.3.5",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.3.2",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0"
    },
    "optionalDependencies": {
        "fsevents": "^2.3.2"
    },
    "resolutions": {
        "graphql": "^15.4.0",
        "graphql-compose": "^7.25.0",
        "webpack": "^5.24.2"
    },
    "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "main": "n/a",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndreBClark/cosmicdivision.git"
    },
    "scripts": {
        "start": "npm i && npm run develop",
        "develop": "gatsby develop --host=0.0.0.0",
        "build": "gatsby build",
        "format": "npm run format-css && npm run format-js",
        "clean": "gatsby clean && rm -rf node_modules && npm i",
        "preflight": "postcss ./src/styles/entry.css -o ../static/preflight.min.css",
        "format-css": "prettier --write src/styles/**/*.{css,scss}",
        "format-js": "prettier --write src/**/*.{js,jsx}",
        "serve": "gatsby serve"
    }
}
