{
    "name": "cosmicdivision-gatsbyblog",
    "private": true,
    "description": "A personal blog starter for a blog powered by Netlify CMS, based on the default blog starter",
    "version": "0.1.0",
    "author": "Andre Clark <andrek@cosmicdivision.dev>",
    "bugs": {
        "url": "https://github.com/AndreBClark/cosmicdivision/issues"
    },
    "dependencies": {
        "@emotion/styled": "^11.3.0",
        "@openfonts/fira-code_latin": "^1.44.8",
        "@tailwindcss/forms": "^0.3.3",
        "@tailwindcss/typography": "^0.4.1",
        "gatsby": "^3.10.1",
        "gatsby-plugin-emotion": "^6.9.0",
        "gatsby-plugin-image": "^1.10.0",
        "gatsby-plugin-manifest": "^3.10.0",
        "gatsby-plugin-module-resolver": "^1.0.3",
        "gatsby-plugin-netlify": "^3.10.0",
        "gatsby-plugin-offline": "^4.10.0",
        "gatsby-plugin-postcss": "^4.10.0",
        "gatsby-plugin-preact": "^5.10.0",
        "gatsby-plugin-react-helmet": "^4.10.0",
        "gatsby-plugin-react-svg": "^3.0.1",
        "gatsby-plugin-sharp": "^3.10.2",
        "gatsby-plugin-transition-link": "^1.20.5",
        "gatsby-remark-images": "^5.7.0",
        "gatsby-remark-prismjs": "^5.7.0",
        "gatsby-remark-vscode": "^3.3.0",
        "gatsby-source-filesystem": "^3.10.0",
        "gatsby-transformer-remark": "^4.7.0",
        "gatsby-transformer-sharp": "^3.10.0",
        "graphql-compose": "^9.0.2",
        "gsap": "^3.7.1",
        "preact": "^10.5.14",
        "preact-render-to-string": "^5.1.19",
        "prismjs": "^1.24.1",
        "tailwindcss": "^2.2.7",
        "twin.macro": "^2.6.2",
        "typeface-fira-sans": "1.1.13",
        "typeface-orbitron": "^1.1.3"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@emotion/react": "^11.4.0",
        "autoprefixer": "^10.3.1",
        "babel-eslint": "^10.0.1",
        "babel-plugin-module-resolver": "^4.1.0",
        "dotenv": "^10.0.0",
        "eslint": "7.31.0",
        "postcss": "^8.3.6",
        "postcss-cli": "^8.3.1",
        "prettier": "^2.3.2",
        "prettier-eslint": "^12.0.0",
        "prettier-eslint-cli": "^5.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "typescript": "4.3.5"
    },
    "resolutions": {
        "graphql": "^15.4.0",
        "graphql-compose": "^9.0.2",
        "webpack": "^5.24.2"
    },
    "homepage": "https://github.com/gatsbyjs/gatsby-starter-blog#readme",
    "keywords": [
        "gatsby"
    ],
    "license": "MIT",
    "main": "n/a",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/AndreBClark/cosmicdivision.git"
    },
    "scripts": {
        "start": "yarn && yarn run develop",
        "develop": "gatsby develop --host=0.0.0.0",
        "build": "gatsby build",
        "build:netlify": "netlify build && yarn run serve",
        "format": "yarn run format-css && npm run format-js",
        "clean": "gatsby clean && rm -rf node_modules && yarn && yarn run compile-css",
        "compile-css": "postcss src/styles/*.css  --dir ./static --ext min.css",
        "format-css": "prettier --write src/styles/**/*.{css,scss}",
        "format-js": "prettier --write src/**/*.{js,jsx}",
        "serve": "gatsby serve --host=0.0.0.0"
    }
}
